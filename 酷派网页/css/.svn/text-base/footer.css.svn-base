*{margin: 0;padding: 0;}
#footer_wrap{
	width: 100%;
	min-width: 1300px;
	/*overflow: hidden;*/
}
#footer{
	width: 100%;
	height: 466px;
}
#footer_con{
	width: 1200px;
	height: 466px;
	margin: 0 auto;
}
#footer_top{
	width: 1160px;
	height: 100px;
	padding-top: 30px;
	padding-bottom: 27px;
	box-sizing: border-box;
	border-bottom: 1px solid #eeeeee;
}
#footer #footer_con ul{
	height: 40px;
	list-style: none;
	font-size: 24px;
}
#footer_con ul li{
	width: 286px;
	height: 40px;
	border-right: 1px solid #eeeeee;
	float: left;
	color: #666666;
}
#footer_con ul li:nth-last-of-type(1){
	border-right: 1px solid transparent;
}
#footer_con ul li img{
	vertical-align: middle;
	margin-right: 22px;
	margin-left: 40px;
	box-sizing: border-box;
}
#footer_centre{
	width: 1160px;
	height: 244px;
	border-bottom: 1px solid #eeeeee;
}
#footer_centre_left{
	height: 200px;
	float: left;
	padding: 30px;
	box-sizing: border-box;
}
#footer_centre_left table{
	width: 600px;
	height: 200px;
	font-size: 14px;
}
th{
	color: #333333;
}
td{
	color: #999999;
	cursor: pointer;
}
td:hover{
	color: #333333;
}
#footer_centre_right{
	width: 350px;
	height: 200px;
	float: right;
	padding: 40px;
	box-sizing: border-box;
}
#number{
	font-size: 23px;
	font-weight: bold;
}
#time{
	font-size: 14px;
	color: #999999;
}
#btn{
	width: 130px;
	height: 50px;
	background: white;
	border: 1px solid #55a0ff;
	color: #55a0ff;
	border-radius: 5px;
	margin-top: 30px;
	margin-left:100px ;
	cursor: pointer;
}
#btn:hover{
	background: #55a0ff;
	color: white;
}
#footer_bottom{
	width: 1160px;
	height: 110px;
	font-size: 12px;
}
#footer_bottom #footer_bottom_first{
	text-align: center;
	padding-top: 30px;
	box-sizing: border-box;
} 
#footer_bottom_second{
	text-align: center;
	padding-top: 20px;
	cursor: pointer;
}
#footer_bottom_second img{
	width: 90px;
	height: 30px;
	vertical-align: middle;
}
